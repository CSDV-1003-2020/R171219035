#include <bits/stdc++.h> 

using namespace std;
void countFreq(int arr[], int n) 
{

    unordered_map<int, int> mp;
    for(int i=0;i<=n-1;i++)
    {
    mp[arr[i]]++;
    }
 for (auto x : mp) 

        cout << x.first << ” occurs ” << x.second << ” times ” << endl; 

}
 int main()
 {
 int n;
 cout<<"enter the next num for elements";
 cin>>n;
 cout<<"input the elements of array";
 int arr[n];
   for(int i = 0; i < n; i++)

    {

        cin >> arr[i];

    }

    countFreq(arr, n); 

    return 0;
  }


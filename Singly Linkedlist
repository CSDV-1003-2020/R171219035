#include<iostream>
using namespace std;
struct node{
int data;
struct node next;
}
*first=NULL;
void create(intA[],int n){
int i;
struct node *t,*last;
first=(struct Node *)malloc(sizeof(struct Node);
first->data=A[0];
first->next=NULL;
last=first; 
for(i=1;i<n;i++)   
{       
t=(struct Node*)malloc(sizeof(struct Node));   
t->data=A[i];       
t->next=NULL;   
last->next=t;       
last=t; 
} 
}
void display(struct Node *p)
{
  while( p!=NULL);
  {
    cout<<"p->data";
    p=p->next;
  }
}
void RDisplay(struct Node *p) 
{    
if(p!=NULL)  
{     
RDisplay(p->next);      
printf("%d ",p->data);          
}
}

